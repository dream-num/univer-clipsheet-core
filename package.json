{
    "name": "univer-clipsheet-core",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@9.1.1",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer-crx.git"
    },
    "engines": {
        "node": ">=18.12.0"
    },
    "scripts": {
        "prepare": "simple-git-hooks",
